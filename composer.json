{
    "name": "saintxak/json-rpc-bundle",
    "description": "The bundle for easy using json-rpc api on your project",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Maystrenko",
            "email": "ashterix69@gmail.com"
        }
    ],
    "type" : "symfony-bundle",
    "homepage": "https://github.com/UFO-CMS/json-rpc-bundle",
    "keywords" : ["ufo", "symfony", "api", "bundle", "json-rpc"],
    "minimum-stability" : "dev",
    "require" : {
        "php": ">=5.6",
        "zendframework/zend-server": ">=2.6",
        "zendframework/zend-json-server": ">=3.0",
        "zendframework/zend-json": ">=2.6",
        "zendframework/zend-http": ">=2.5"
    },
    "autoload" : {
        "psr-4" : { "Ufo\\JsonRpcBundle\\" : "src/Ufo/JsonRpcBundle/" }
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "1.0.x-dev"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
